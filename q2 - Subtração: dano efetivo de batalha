#include <stdio.h>

int main() {
int a[5], b[5], p[5];
int x;
    printf("Digite 6 valores de ataque para (a), um em baixo do outro!:\\n");
    for(x = 0; x < 6; x++){
        scanf("%d", &a[x]);
    }
    printf("Digite 6 valores de defesa para (b), um em baixo do outro!:\\n");
    for(x = 0; x < 6; x++){
        scanf("%d", &b[x]);
    }
    for(x = 0; x < 6; x++){
        p[x] = a[x] - b[x];
    }
    printf("\\nO dano causadom em (p) Ã©:\\n");
    for(x = 0; x < 6; x++){
        printf("%d", p[x]);
    }
return 0;
}
